{
  "name": "@shell-platform/e2e-tests",
  "version": "1.0.0",
  "description": "Comprehensive end-to-end tests for Shell Platform",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:auth": "playwright test tests/auth/",
    "test:core": "playwright test tests/core/",
    "test:plugins": "playwright test tests/plugins/",
    "test:api": "playwright test tests/api/",
    "test:visual": "playwright test tests/visual/",
    "test:performance": "playwright test tests/performance/",
    "test:security": "playwright test tests/security/",
    "test:smoke": "playwright test --grep @smoke",
    "test:critical": "playwright test --grep @critical",
    "test:regression": "playwright test --grep @regression",
    "report": "playwright show-report",
    "report:open": "playwright show-report --open",
    "install-browsers": "playwright install",
    "install-deps": "playwright install-deps",
    "codegen": "playwright codegen",
    "trace": "playwright show-trace"
  },
  "dependencies": {
    "@playwright/test": "^1.40.0",
    "dotenv": "^16.3.1",
    "faker": "^5.5.3",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.0",
    "sharp": "^0.32.6",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/faker": "^5.5.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.4",
    "typescript": "^5.2.0"
  },
  "keywords": [
    "playwright",
    "e2e",
    "testing",
    "shell-platform",
    "automation",
    "qa"
  ],
  "author": "Shell Platform QA Team",
  "license": "MIT"
}